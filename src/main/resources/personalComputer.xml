<?xml version="1.0" encoding="UTF-8"?>
<st:personalComputer xmlns:st="http://www.example.com/personalComputer"
                     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:schemaLocation="http://www.example.com/personalComputer personalComputer.xsd">

    <computerPart id="001">
        <name>CPU</name>
        <country>CHINA</country>
        <maker>AMD_FX8200</maker>
        <parametersCPU>
            <quntityCore>8</quntityCore>
            <frequency>4.2</frequency>
            <cacheMemory>3</cacheMemory>
        </parametersCPU>
        <price>242</price>
    </computerPart>

    <computerPart id="002">
        <name>Drive</name>
        <country>CHINA</country>
        <maker>Samsung</maker>
        <parameterDrive>
            <interfaces>SATA_II</interfaces>
        </parameterDrive>
        <price>42</price>
    </computerPart>

    <computerPart id="003">
        <name>Keybord</name>
        <country>TAIBAI</country>
        <maker>Hi tech</maker>
        <parameterKeybord>
            <type>WIRElESS</type>
        </parameterKeybord>
        <price>12</price>
    </computerPart>

    <computerPart id="004">
        <name>Monitor</name>
        <country>MALAYSIA</country>
        <maker>BENQ</maker>
        <parameterMonitor>
            <sizeScreen>17</sizeScreen>
        </parameterMonitor>
        <price>300</price>
    </computerPart>

    <computerPart id="005">
        <name>Mouse</name>
        <country>CHINA</country>
        <maker>HP</maker>
        <parametersMouse>
            <typeMouse>optical</typeMouse>
        </parametersMouse>
        <price>11</price>
    </computerPart>

    <computerPart id="006">
        <name>Mouse</name>
        <country>CHINA</country>
        <maker>HP</maker>
        <parametersMouse>
            <typeMouse>laser</typeMouse>
        </parametersMouse>
        <price>11</price>
    </computerPart>

    <computerPart id="007">
        <name>Videocard</name>
        <country>MALAYSIA</country>
        <maker>Radeon_HD7990</maker>
        <parametersVideocard>
            <extensionBoard>PCI-Ex</extensionBoard>
            <dataBus>256</dataBus>
            <versionShader>5.0</versionShader>
            <videoMemory>2</videoMemory>
        </parametersVideocard>
        <price>342</price>
    </computerPart>

</st:personalComputer>
